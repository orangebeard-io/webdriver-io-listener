{
  "name": "@orangebeard-io/wdio-orangebeard-reporter",
  "version": "1.0.1",
  "description": "A reporter and service to send WDIO test output to Orangebeard",
  "main": "src/index",
  "scripts": {
    "get-version": "echo $npm_package_version",
    "update-version": "release-it --ci --no-git --no-npm.publish",
    "create-changelog": "auto-changelog --template changelog-template.hbs --starting-version v$npm_package_version"
  },
  "keywords": [
    "wdio-reporter",
    "wdio-plugin",
    "wdio-service",
    "orangebeard"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/orangebeard-io/webdriver-io-listener.git"
  },
  "author": "Team Soju",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/orangebeard-io/webdriver-io-listener/issues"
  },
  "homepage": "https://github.com/orangebeard-io/webdriver-io-listener#readme",
  "dependencies": {
    "@wdio/reporter": "7.19.0",
    "@wdio/types": "7.19.0",
    "@orangebeard-io/javascript-client": "1.0.9",
    "strip-color": "0.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "auto-changelog": "2.4.0",
    "release-it": "14.13.1"
  }
}
